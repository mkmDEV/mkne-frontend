# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true
ij_html_quote_style = double
ij_html_space_inside_empty_tag = true

[*.md]
max_line_length = off
trim_trailing_whitespace = false
